{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "title": "Florida Coastal Prep",
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1400px"
    },
    "color": {
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "palette": [
        {
          "name": "Base (White)",
          "slug": "base",
          "color": "#FFFFFF"
        },
        {
          "name": "Contrast (Navy)",
          "slug": "contrast",
          "color": "#0A192F"
        },
        {
          "name": "Primary Gold",
          "slug": "primary",
          "color": "#FBBF24"
        },
        {
          "name": "Secondary Navy",
          "slug": "secondary",
          "color": "#112240"
        },
        {
          "name": "Deep Space",
          "slug": "tertiary",
          "color": "#020C1B"
        },
        {
          "name": "Light Gray",
          "slug": "light-gray",
          "color": "#F1F5F9"
        }
      ],
      "gradients": [
        {
          "name": "Navy to Deep Space",
          "slug": "navy-deep",
          "gradient": "linear-gradient(180deg, #112240 0%, #020C1B 100%)"
        },
        {
          "name": "Gold to Transparent",
          "slug": "gold-trans",
          "gradient": "linear-gradient(180deg, rgba(251, 191, 36, 0.2) 0%, rgba(251, 191, 36, 0) 100%)"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "customLineHeight": true,
      "dropCap": true,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Bebas Neue\", sans-serif",
          "name": "Display (Bebas)",
          "slug": "display",
          "fontFace": [
            {
              "fontFamily": "Bebas Neue",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/bebas-neue-v14-latin.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "\"Oswald\", sans-serif",
          "name": "Heading (Oswald)",
          "slug": "heading",
          "fontFace": [
            {
              "fontFamily": "Oswald",
              "fontWeight": "200 700",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/oswald-v53-latin.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Body (Inter)",
          "slug": "sans",
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontWeight": "100 900",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/inter-v13-latin.woff2"
              ]
            },
            {
              "fontFamily": "Inter",
              "fontWeight": "100 900",
              "fontStyle": "italic",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/inter-v13-latin-italic.woff2"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "name": "Small",
          "slug": "small",
          "size": "0.875rem"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "1rem"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "1.25rem"
        },
        {
          "name": "Extra Large",
          "slug": "x-large",
          "size": "1.5rem"
        },
        {
          "name": "Huge",
          "slug": "huge",
          "size": "clamp(2.5rem, 5vw, 4rem)"
        },
        {
          "name": "Gigantic",
          "slug": "gigantic",
          "size": "clamp(4.5rem, 8vw, 10rem)"
        }
      ]
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ],
      "spacingSizes": [
        {
          "name": "Extra Small",
          "slug": "10",
          "size": "0.5rem"
        },
        {
          "name": "Small",
          "slug": "20",
          "size": "0.75rem"
        },
        {
          "name": "Medium Small",
          "slug": "30",
          "size": "1rem"
        },
        {
          "name": "Medium",
          "slug": "40",
          "size": "1.5rem"
        },
        {
          "name": "Medium Large",
          "slug": "50",
          "size": "2.25rem"
        },
        {
          "name": "Large",
          "slug": "60",
          "size": "3.375rem"
        },
        {
          "name": "Extra Large",
          "slug": "70",
          "size": "5rem"
        },
        {
          "name": "Huge",
          "slug": "80",
          "size": "7.5rem"
        }
      ]
    },
    "shadows": {
      "presets": [
        {
          "name": "Natural",
          "slug": "natural",
          "shadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)"
        },
        {
          "name": "Deep",
          "slug": "deep",
          "shadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)"
        },
        {
          "name": "Gold Glow",
          "slug": "gold-glow",
          "shadow": "0 0 15px rgba(251, 191, 36, 0.3)"
        },
        {
          "name": "Soft",
          "slug": "soft",
          "shadow": "0 4px 12px rgba(0, 0, 0, 0.05)"
        },
        {
          "name": "Elevated",
          "slug": "elevated",
          "shadow": "0 10px 25px rgba(0, 0, 0, 0.1)"
        },
        {
          "name": "XL",
          "slug": "xl",
          "shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)"
        },
        {
          "name": "2XL",
          "slug": "2xl",
          "shadow": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--contrast)",
      "text": "var(--wp--preset--color--base)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sans)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.6"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          }
        }
      },
      "button": {
        "border": {
          "radius": "0px"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--secondary)"
        },
        "typography": {
          "fontWeight": "700",
          "textTransform": "uppercase",
          "letterSpacing": "0.1em"
        },
        "spacing": {
          "padding": {
            "top": "1rem",
            "bottom": "1rem",
            "left": "2rem",
            "right": "2rem"
          }
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)"
        }
      }
    },
    "spacing": {
      "padding": {
        "top": "0px",
        "right": "0px",
        "bottom": "0px",
        "left": "0px"
      }
    },
    "blocks": {
      "core/navigation": {
        "typography": {
          "fontSize": "0.675rem",
          "fontWeight": "700",
          "letterSpacing": "0.2em",
          "textTransform": "uppercase"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--display)"
        }
      },
      "core/query-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--display)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--display)",
          "fontWeight": "400"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    },
    {
      "name": "comments",
      "title": "Comments",
      "area": "uncategorized"
    }
  ],
  "patterns": [
    "fl-coastal-prep/hero",
    "fl-coastal-prep/stats",
    "fl-coastal-prep/grid",
    "fl-coastal-prep/cta",
    "fl-coastal-prep/section-header",
    "fl-coastal-prep/faculty-grid",
    "fl-coastal-prep/campus-showcase",
    "fl-coastal-prep/programs-hero",
    "fl-coastal-prep/programs-detail",
    "fl-coastal-prep/blueprint-gallery",
    "fl-coastal-prep/donors-grid",
    "fl-coastal-prep/donors-showcase",
    "fl-coastal-prep/news-archive",
    "fl-coastal-prep/schedule-results",
    "fl-coastal-prep/contact-form",
    "fl-coastal-prep/apply-form"
  ]
}
