          "slug": "display"
        },
        {
          "fontFamily": "\"Oswald\", sans-serif",
          "name": "Heading (Oswald)",
          "slug": "heading"
        },
        {
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Body (Inter)",
          "slug": "sans"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "spacing": {
      "customPadding": true,
      "customMargin": true
    },
    "border": {
      "customColor": true,
      "customRadius": true,
      "customStyle": true,
      "customWidth": true
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--contrast)",
      "text": "var(--wp--preset--color--base)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sans)",
      "fontSize": "1rem",
      "lineHeight": "1.6"
    }
  }
