{
  "version": 2,
  "title": "Florida Coastal Prep",
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "custom": true,
      "customDuotone": true,
      "palette": [
        {
          "name": "Base",
          "slug": "base",
          "color": "#FFFFFF"
        },
        {
          "name": "Contrast",
          "slug": "contrast",
          "color": "#0A192F"
        },
        {
          "name": "Primary Gold",
          "slug": "primary",
          "color": "#FBBF24"
        },
        {
          "name": "Secondary Navy",
          "slug": "secondary",
          "color": "#112240"
        },
        {
          "name": "Deep Space",
          "slug": "tertiary",
          "color": "#020C1B"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "customLineHeight": true,
      "dropCap": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Bebas Neue\", sans-serif",
